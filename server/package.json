{
  "name": "job-importer-server",
  "version": "1.0.0",
  "description": "Scalable Job Importer Backend with Queue Processing",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "worker": "ts-node-dev --respawn --transpile-only src/workers/index.ts",
    "init-db": "ts-node-dev --transpile-only src/scripts/initDatabase.ts"
  },
  "keywords": ["job-importer", "queue", "bullmq", "mongodb"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "bullmq": "^5.1.0",
    "ioredis": "^5.3.2",
    "axios": "^1.6.2",
    "xml2js": "^0.6.2",
    "node-cron": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/xml2js": "^0.4.14",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0"
  }
}

